{
  "name": "ChatGPT Assistant",
  "icon": "openai-icon.svg",
  "identifier": "com.da.popclip.extension.chatgpt",
  "description": "A ChatGPT Assistant",
  "note": "You will need an <a href=\"https://platform.openai.com/account/api-keys\">OpenAI API key</a>",
  "app": {
    "name": "ChatGPT",
    "link": "https://openai.com/blog/chatgpt"
  },
  "popclipVersion": 4069,
  "module": "chatgpt.js",
  "entitlements": [
    "network"
  ],
  "options": [
    {
      "identifier": "apiUrl",
      "label": "API Url",
      "type": "string"
    },
    {
      "identifier": "apikey",
      "label": "API Key",
      "type": "string"
    },
    {
      "identifier:": "model",
      "label": "Model",
      "type": "multiple",
      "defaultValue": "gpt-3.5-turbo",
      "values": ["gpt-3.5-turbo", "gpt-4-turbo-preview", "gpt-4", "llama3-70b-8192", "llama3-8b-8192", "deepseek-chat"]
    },
    {
      "identifier": "prompt",
      "label": "Assistant",
      "type": "string"
    }
  ]
}
